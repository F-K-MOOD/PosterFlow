  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "prepare": "husky",
    "lint:es": "eslint src/**/*.{ts,vue}",
    "lint:style": "stylelint src/**/*.vue",
    "spellcheck": "cspell lint --dot --cache --color --show-suggestions --gitignore \"src/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md|vue)\"",
    "commit": "git-cz",
    "preview": "vite preview",
    "dev:all": "concurrently \"npm run dev\" \"cd server && npm run dev\""
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
