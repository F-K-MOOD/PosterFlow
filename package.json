    "@commitlint/config-conventional": "20.2.0",
    "@eslint/js": "9.7.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "4.3.1",
    "commitlint": "20.2.0",
    "concurrently": "^9.2.1",
    "cspell": "9.4.0",
    "cz-git": "1.12.0",
    "eslint": "9.7.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-vue": "9.27.0",
    "husky": "9.1.7",
    "less": "^4.5.1",
    "sass-embedded": "^1.97.1",
    "stylelint": "16.23.0",
    "stylelint-config-recommended": "17.0.0",
