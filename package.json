    "array-move": "^4.0.0",
    "axios": "^1.13.2",
    "clipboard": "^2.0.11",
    "core-js": "3.6.5",
    "cropperjs": "1.5.11",
    "file-saver": "2.0.5",
    "hotkeys-js": "^4.0.0",
    "html2canvas": "^1.4.1",
    "lego-bricks": "^1.0.11",
    "lodash-es": "4.17.22",
    "path-to-regexp": "^8.3.0",
    "pinia": "^3.0.4",
    "qrcode": "1.4.4",
    "uuid": "^13.0.0",
    "vue": "3.5.24",
    "vue-router": "4.6.4",
    "vuedraggable": "4.0.1"
